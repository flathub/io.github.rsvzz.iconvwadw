{
  "id": "io.github.rsvzz.iconvwadw",
  "runtime": "org.gnome.Platform",
  "runtime-version": "49",
  "sdk": "org.gnome.Sdk",
  "command": "iconvwadw",
  "sdk-extensions": [
    "org.freedesktop.Sdk.Extension.rust-stable"
  ],
  "build-options": {
    "append-path": "/usr/lib/sdk/rust-stable/bin",
    "env": {
      "HOME_CARGO": "/run/build/csven/cargo"
    }
  },
  "finish-args": [
    "--share=ipc",
    "--device=dri",
    "--socket=fallback-x11",
    "--socket=wayland"
  ],
  "cleanup": [
    "/include"
  ],
  "modules": [
    {
      "name": "iconvwadw",
      "buildsystem": "meson",
      "sources": [
        {
          "type": "archive",
          "url": "https://github.com/rsvzz/iconvwadw-rs/releases/download/${CRAFT_PROJECT_VERSION}/iconvwadw-rs_all.tar.xz",
          "sha256": "f707989b9a9db2b00231ad74a418a3359f8753a8f53f9c7f7ff203c15e4130c3"
        }
      ]
    }
  ]
}